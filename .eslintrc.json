{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types":  [0],
        "react/no-array-index-key": [0],
        "max-len": "off"
    }
}
